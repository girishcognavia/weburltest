# Server Configuration
PORT=3000
NODE_ENV=development

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Cache Configuration
CACHE_TTL=300

# Proxy Configuration
MAX_PROXY_TIMEOUT=10000
REQUEST_SIZE_LIMIT=10mb

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
PREVIEW_RATE_LIMIT_PER_HOUR=10

# Security
ALLOWED_DOMAINS=*
BLACKLISTED_DOMAINS=

# Logging
LOG_LEVEL=info

# Widget Configuration
WIDGET_CDN_URL=https://cdn.yourcompany.com/widget.js
